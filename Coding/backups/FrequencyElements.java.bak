    public static int[] countFrequency(int[] numbers) {
        int maxNumber = Arrays.stream(numbers).max().orElse(0);
        int[] frequency = new int[maxNumber + 1];

        for (int number : numbers) {
            frequency[number]++;
        }

        return frequency;
    }

    public static void main(String[] args) {
        int[] numbers = getNumbers();
        int[] frequency = countFrequency(numbers);

        System.out.println("Numbers: " + Arrays.toString(numbers));
        System.out.println("Frequency: " + Arrays.toString(frequency));
    }
}
