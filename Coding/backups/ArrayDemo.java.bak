public class ArrayDemo {

    public static void main(String[] args) {
        int[] numbers = { 1, 3, 5, 7, 9, 2, 4, 6, 8, 10 };
        System.out.println("Array : Numbers : - ");
        for (int i = 0; i <= numbers.length - 1; i++) {
            System.out.print(numbers[i] + " ");
        }
    }
}
